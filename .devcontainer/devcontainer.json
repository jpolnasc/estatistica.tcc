{
    "name": "Algoritmos de Aprendizado Online para Previsão de Séries Temporais",
    "dockerFile": "Dockerfile",
    "settings": { 
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "build": {
        "docker": {
            "buildx": false
        }
    },
    "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter",
        "yhat.r-language",
        "RandomFractalsInc.vscode-data-preview",
        "reditorsupport.r",
        "ms-python.vscode-pylance", 
        "ms-python.flake8",
        "ms-toolsai.vscode-jupyter-cell-tags", 
        "ms-toolsai.jupyter", "ms-python.black-formatter"
    ],
    "runArgs": [
        "--network=timescale-tcc_rede-tcc"
    ]
}
